<p id="notice"><%= notice %></p>

<h1>Word Responses</h1>

<table>
  <thead>
    <tr>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @word_responses.each do |word_response| %>
      <tr>
        <td><%= link_to 'Show', word_response %></td>
        <td><%= link_to 'Edit', edit_word_response_path(word_response) %></td>
        <td><%= link_to 'Destroy', word_response, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Word Response', new_word_response_path %>
